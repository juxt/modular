                        :resource-prefix "\"META-INF/resources/webjars/bootstrap/3.3.0\""
                        :key :bootstrap-resources]}}
   :dependencies {[:modular-bidi-router :webrouter] {:twitter-bootstrap :service}}}

  {:module :bootstrap-cover-resources
